{
	"name": "we-task-react",
	"version": "1.0.0",
	"homepage": "https://vidriktt.github.io/we-task-react",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"lint": "eslint .",
		"prettier": "prettier --write '**/*.{js,json,css,html}'"
	},
	"eslintConfig": {
		"extends": [
			"react-app"
		],
		"rules": {
			"react/prop-types": "off"
		}
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.26.2",
		"react-scripts": "5.0.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.11.1",
		"eslint": "^8.57.1",
		"eslint-plugin-react": "^7.37.0",
		"gh-pages": "^6.1.1",
		"globals": "^15.9.0",
		"prettier": "^3.3.3",
		"tailwindcss": "^3.4.13"
	}
}
